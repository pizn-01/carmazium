{
    "version": 2,
    "builds": [
        {
            "src": "backend/src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/listings/(.*)",
            "dest": "backend/src/main.ts"
        },
        {
            "src": "/(.*)",
            "dest": "backend/src/main.ts"
        }
    ]
}