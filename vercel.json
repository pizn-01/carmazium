{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/:match*",
            "destination": "/backend/src/main.ts"
        }
    ],
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        },
        {
            "src": "backend/src/main.ts",
            "use": "@vercel/node"
        }
    ]
}